
augroup MyAutoCmd
	autocmd!
augroup END

syntax on
colorscheme desert

set backspace=indent,eol,start
set clipboard=unnamedplus
set nowritebackup
set nobackup
set noswapfile
set mouse=a
set number
set scrolloff=5
set laststatus=2
set cmdheight=1
set shiftwidth=3
set tabstop=3
set showmatch
set matchtime=3
set cindent
set textwidth=0


